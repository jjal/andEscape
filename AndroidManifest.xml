        <activity android:name="com.futuresandwich.game.DirtBike.Splash1Activity"></activity>
        <activity android:name="com.futuresandwich.game.DirtBike.Splash2Activity"></activity>
        <activity android:name="com.futuresandwich.game.DirtBike.Splash3Activity"></activity>
        <activity android:name="com.futuresandwich.game.DirtBike.Splash4Activity"></activity>
